connect --user=region1dataAdmin --password=123

list members
describe member --name=locator

list regions
describe region --name=region1
describe region --name=region2

put --key="region1dataAdmin-key1" --value="region1dataAdmin1-value1" --region=/region1
put --key="region1dataAdmin-key2" --value="region1dataAdmin1-value2" --region=/region2

query --query="select * from /region1"
query --query="select * from /region2"

gc

change loglevel --loglevel=DEBUG --members=server1

export cluster-configuration --zip-file-name=region1dataAdmin-config.zip
